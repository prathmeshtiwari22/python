num=int(input("enter the number"))
if num>=5 :
    print(" the number is greater than 5")
else :
    print(" you are smaller than 5")

char=str(input("enter the character "))
if char=="a" :
    print(" the character is a")
elif char=="b" :
    print(" the character is b")
else :
    print(" other than a and b ")
for i in range(0,5) :
    print(i)
